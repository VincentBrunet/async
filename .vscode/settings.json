{
  "deno.enable": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.autoIndent": "advanced",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.formatOnSave": true
  },
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "__nullptr": "cpp",
    "cstddef": "cpp",
    "type_traits": "cpp",
    "cstdlib": "c",
    "stdio.h": "c",
    "typeinfo": "c",
    "__hash_table": "c",
    "__split_buffer": "c",
    "array": "c",
    "bitset": "c",
    "initializer_list": "c",
    "iterator": "c",
    "string": "c",
    "string_view": "c",
    "unordered_map": "c",
    "utility": "c",
    "vector": "c"
  },
  "deno.lint": true,
  "deno.unstable": true,
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "editor.rulers": [
    { "column": 0, "color": "#5a5a5a80" },
    { "column": 80, "color": "#5a5a5a80" }
  ]
}
