{
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.formatOnSave": true
  },
  "files.associations": {
    "__nullptr": "cpp",
    "cstddef": "cpp",
    "type_traits": "cpp",
    "cstdlib": "c",
    "stdio.h": "c"
  }
}
